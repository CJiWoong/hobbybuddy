<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC	"-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="kr.co.hb.group.dao.OpenChatDAO">
	
	<insert id="openlist" parameterType="HashMap">
		INSERT INTO openlist(gidx, id) VALUES(#{gidx}, #{loginId})
	</insert>
	
	<select id="openchatlist" resultType="kr.co.hb.group.dto.OpenChatDTO">
		SELECT * FROM openlist
	</select>
</mapper>